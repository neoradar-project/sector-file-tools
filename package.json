{
    "name": "sector-file-tools",
    "version": "1.2.4",
    "description": "Utilities for working with SCT, ASR and ESE files.",
    "main": "dist/src/index.js",
    "type": "module",
    "types": "dist/src/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/src/index.js",
            "require": "./dist/src/index.js",
            "default": "./dist/src/index.js",
            "types": "./dist/src/index.d.ts"
        }
    },
    "files": [
        "dist/",
        "index.js"
    ],
    "scripts": {
        "test": "mocha",
        "build": "tsc",
        "build:prod": "tsc -p tsconfig.prod.json",
        "lint": "prettier --check .",
        "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
        "prepare": "exit 0",
        "prepublishOnly": "npm run build:prod",
        "prepack": "npm run build:prod",
        "release": "npm run build:prod && git add dist && git commit -m \"Build: Update pre-built distribution files\""
    },
    "author": "Contributors to sector-file-tools",
    "repository": {
        "type": "git",
        "url": "https://github.com/eirslett/sector-file-tools.git"
    },
    "prettier": {
        "singleQuote": true
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "homepage": "https://github.com/eirslett/sector-file-tools",
    "license": "MIT",
    "devDependencies": {
        "prettier": "^2.8.3",
        "eslint": "^8.33.0"
    },
    "dependencies": {
        "@turf/projection": "^7.2.0",
        "typescript": "^5.8.3",
        "@types/node": "^22.0.0",
        "@types/chai": "^4.3.4",
        "@types/chai-almost": "^1.0.1",
        "@types/geojson": "^7946.0.10",
        "@types/mocha": "^10.0.10",
        "ts-node": "^10.9.1",
        "chai": "^4.3.7",
        "chai-almost": "^1.0.1",
        "mocha": "^11.0.0"
    }
}
